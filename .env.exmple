OEP_API_TOKEN_LOCAL=12341234124123412341234...

# Data directory name 
OEP_OEM_FILE=data/datapackages/example/oed_example.json

# Optional overrides
LOG_LEVEL=DEBUG
HTTP_TIMEOUT=30
